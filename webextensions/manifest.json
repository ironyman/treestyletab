{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "2.0",
  "author": "YUKI \"Piro\" Hiroshi",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "activeTab",
    "bookmarks",
    "contextMenus",
    "contextualIdentities",
    "cookies",
    "notifications",
    "sessions",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "resources/16x16-light.svg",
    "20": "resources/20x20-light.svg",
    "24": "resources/24x24-light.svg",
    "64": "resources/icon64.png",
    "94": "resources/icon94.png"
  },
  "background": {
    "page": "background/background.html"
  },
  "browser_action": {
    "default_title": "__MSG_sidebarTitle__",
    "default_icon": {
      "16": "resources/16x16-light.svg",
      "20": "resources/20x20-light.svg",
      "24": "resources/24x24-light.svg"
    },
    "browser_style": true
  },
  "sidebar_action": {
    "default_title": "__MSG_sidebarTitle__",
    "default_icon": {
      "16": "resources/16x16-light.svg",
      "20": "resources/20x20-light.svg",
      "24": "resources/24x24-light.svg"
    },
    "default_panel": "sidebar/sidebar.html",
    "browser_style": true
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": { "default": "F1" },
      "description": "__MSG_sidebarToggleDescription__"
    },
    "tab_go_prev": {
      "suggested_key": { "default": "Ctrl+Shift+Up" },
      "description": "__MSG_sidebarToggleDescription__"
    },
    "tab_go_next": {
      "suggested_key": { "default": "Ctrl+Shift+Down" },
      "description": "__MSG_sidebarToggleDescription__"
    },
    "tab_go_parent": {
      "suggested_key": { "default": "Ctrl+Shift+Left" },
      "description": "__MSG_sidebarToggleDescription__"
    },
    "tab_go_child": {
      "suggested_key": { "default": "Ctrl+Shift+Right" },
      "description": "__MSG_sidebarToggleDescription__"
    },
    "tab_move_prev": {
      "suggested_key": { "default": "Alt+Shift+Up" },
      "description": "__MSG_sidebarToggleDescription__"
    },
    "tab_move_next": {
      "suggested_key": { "default": "Alt+Shift+Down" },
      "description": "__MSG_sidebarToggleDescription__"
    },
    "tab_move_parent": {
      "suggested_key": { "default": "Alt+Shift+Left" },
      "description": "__MSG_sidebarToggleDescription__"
    },
    "tab_move_child": {
      "suggested_key": { "default": "Alt+Shift+Right" },
      "description": "__MSG_sidebarToggleDescription__"
    }
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },
  "web_accessible_resources": [
    "resources/group-tab.html*"
  ],
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "treestyletab@piro.sakura.ne.jp",
      "strict_min_version": "57.0a1"
    }
  }
}
